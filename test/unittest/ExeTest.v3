def test_ref_eq(t: ExeTester) {
	t.extensions |= Extension.GC;

	t.sig(SigCache.rr_i);
	t.code([
		Opcode.LOCAL_GET.code, 0,
		Opcode.LOCAL_GET.code, 1,
		Opcode.REF_EQ.code
	]);

	var nil = Values.REF_NULL, obj1 = Value.Ref(HostObject.new()), obj2 = Value.Ref(HostObject.new());
	t.argsN([nil, nil]).assert2_i(1);
	t.argsN([obj1, nil]).assert2_i(0);
	t.argsN([nil, obj1]).assert2_i(0);
	t.argsN([obj1, obj1]).assert2_i(1);
	t.argsN([obj1, obj2]).assert2_i(0);
	t.argsN([obj2, obj2]).assert2_i(1);
}

