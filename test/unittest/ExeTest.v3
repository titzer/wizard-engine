def make_br_ifN(i: ExeTester, depth: int) {
	i.sig(SigCache.i_i);
	var w = DataWriter.new();
	for (j < depth) {
		w.putbb(Opcode.BLOCK.code, NONE);
	}
	for (j < depth) {
		w.putbb(Opcode.LOCAL_GET.code, 0);
		w.putb(Opcode.I32_CONST.code);
		w.put_sleb32(j);
		w.putb(Opcode.I32_EQ.code);
		w.putb(Opcode.BR_IF.code);
		w.put_sleb32(j);
	}
	for (j < depth) {
		w.putb(Opcode.END.code);
		w.putb(Opcode.I32_CONST.code);
		w.put_sleb32(j + 13);
		w.putb(Opcode.RETURN.code);
	}
	i.codev(w.extract());
}
def test_br_ifN(i: ExeTester) {
	var depth = 65;
	make_br_ifN(i, depth);

	for (j < depth) {
		i.args_i(j).assert2_i(j + 13);
	}
}
def test_br_ifN2(i: ExeTester) {
	var depth = 129;
	make_br_ifN(i, depth);

	for (k = 1; k <= depth; k *= 2) {
		for (d = -3; d < 4; d++) {
			var j = k + d;
			if (j < 0 || j >= depth) continue;
			i.args_i(j).assert2_i(j + 13);
		}
	}
}
