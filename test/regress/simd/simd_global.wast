;; v128.const normal parameter (e.g. (i8x16, i16x8 i32x4, f32x4))

(module
  (global $g1 (mut v128)
    (v128.const i8x16  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF))
  (func (export "get") (result v128) (global.get $g1))
  (func (export "set") (param v128) (global.set $g1 (local.get 0)))
)

(assert_return (invoke "get") (v128.const i8x16  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF  0xFF))

(assert_return (invoke "set"  (v128.const i8x16  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE)))
(assert_return (invoke "get") (v128.const i8x16  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE  0xFE))

(assert_return (invoke "set"  (v128.const i8x16  0xAE  0xBE  0xCE  0xDE  0xEE  0xFE  0x0E  0x1E  0x2E  0x3E  0x4E  0x5E  0x6E  0x7E  0x8E  0x9E)))
(assert_return (invoke "get") (v128.const i8x16  0xAE  0xBE  0xCE  0xDE  0xEE  0xFE  0x0E  0x1E  0x2E  0x3E  0x4E  0x5E  0x6E  0x7E  0x8E  0x9E))

(assert_return (invoke "set" (v128.const i32x4 0_123_456_789 0_123_456_789 0_123_456_789 0_123_456_789)))
(assert_return (invoke "get") (v128.const i32x4 0_123_456_789 0_123_456_789 0_123_456_789 0_123_456_789))

(assert_return (invoke "set" (v128.const i32x4 11 -22 33 -44)))
